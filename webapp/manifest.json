{
    "_version": "1.0.0",
    "sap.app": {
        "id": "health.med-united.earztbrief",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{app.title}}",
        "description": "{{app.description}}",
        "applicationVersion": {
        "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "health.med-united.earztbrief.view.InputForm",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.93",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "health.med-united.earztbrief.i18n.i18n",
                    "supportedLocales": ["DE", "EN"],
                    "fallbackLocale": "EN"
                }
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"}]
        }
    }
}